{
  "name": "frontinsm",
  "version": "2.0.0",
  "description": "Maior evento focado em FrontEnd do interior do estado do Rio Grande do Sul - Brasil",
  "config": {
    "build_path": "build",
    "server_port": "5022",
    "version_message": "Built Front-in SM version"
  },
  "scripts": {
    "webpack": "./node_modules/webpack/bin/webpack.js -p",
    "server": "./node_modules/http-server/bin/http-server -p $npm_package_config_server_port ./build",
    "dev-server": "./node_modules/webpack/bin/webpack.js -w -d",
    "watch": "npm run dev-server",
    "build": "npm run webpack",
    "version:patch": "npm version patch -m \"$npm_package_config_version_message %s\"",
    "version:commit": "git add -A .; git commit -m \"$npm_package_config_version_message $npm_package_version\"",
    "push:git": "git push --tags origin HEAD:master",
    "push:gh-pages": "git subtree push --prefix build origin gh-pages",
    "publish": "npm run build; npm run version:patch; npm run version:commit; npm run push:git; npm run push:gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taltk9/frontinsm.git"
  },
  "keywords": [
    "frontend",
    "front-end",
    "web",
    "conference",
    "evento",
    "Santa Maria",
    "Rio Grande do Sul",
    "Brasil"
  ],
  "author": "Thiago Alves <taltk9@gmail.com> (http://github.com/taltk9)",
  "license": "MIT",
  "homepage": "http://frontinsm.com.br/",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint-plugin-react": "^3.6.2",
    "eval": "^0.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "handlebars": "^4.0.5",
    "handlebars-layouts": "^3.1.3",
    "http-server": "^0.9.0",
    "jsx-loader": "^0.13.2",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "react-to-html-webpack-plugin": "^2.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-evil-icons": "^0.4.0"
  }
}
