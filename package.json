{
  "name": "frontinsm",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/devthiago/frontinsm.git"
  },
  "author": "Guto Foletto <gutofoletto@gmail.com>",
  "scripts": {
    "dev": "parcel src/templates/index.pug --no-cache",
    "build": "parcel build src/templates/index.pug --no-cache",
    "version:patch": "npm version patch -m \"$npm_package_config_version_message %s\"",
    "version:commit": "git add -A .; git commit -m \"$npm_package_config_version_message $npm_package_version\"",
    "push:git": "git push --tags origin HEAD:master",
    "push:gh-pages": "git subtree push --prefix dist origin gh-pages",
    "publish": "npm run build; npm run version:patch; npm run version:commit; npm run push:git; npm run push:gh-pages"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "eslint": "^5.8.0",
    "parcel-bundler": "^1.11.0",
    "pug": "^2.0.3",
    "sass": "^1.17.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "normalize-scss": "^7.0.1"
  }
}
